    "@storybook/testing-library": "0.0.13",
    "@types/node": "^18.14.2",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.48",
    "esbuild-dev-server": "^0.3.0",
    "eslint": "^8.31.0",
